                                    <MenuItem mnemonicParsing="false" text="Print by name" />
                                    <MenuItem mnemonicParsing="false" text="Print by county" />
                                    <MenuItem mnemonicParsing="false" text="Print by expiration date" />
                                    <MenuItem mnemonicParsing="false" text="Print by membership fee" />
                              </items>
                            </Menu>
                            <Menu mnemonicParsing="false" text="Class Schedule">
                              <items>
                                <MenuItem mnemonicParsing="false" text="Print class schedule" />
